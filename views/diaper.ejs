<div class="container">
    <div style="text-align:center;margin-top:20px;margin-bottom:20px;">
        <strong style="color:#22741C;"><%= keyword %></strong> 검색 결과 입니다.
    </div>

    <table class="table table-striped">
<%
        if (wmpDealImg != '') {
            var limit = 5;
            if (wmpDealImg.length < 6) {
                limit = wmpDealImg.length;
            }
            for(var i = 0; i < limit; i++) {
%>
        <tr>
            <td style="text-align:center;vertical-align:middle;"><img src="/images/wmp.png" style="width:50px;"/><br/><%=wmpPrice[i]%></td>
            <td><a href="<%=wmpLinkUrl[i]%>" target="_blank"><img src="<%=wmpDealImg[i]%>" style="width:100px;"/></a></td>
        </tr>
<%
            }
        }

        if (cpDealImg != '') {
            var limit = 5;
            if (cpDealImg.length < 6) {
                limit = cpDealImg.length;
            }
            for(var i = 0; i < limit; i++) {
%>
        <tr>
            <td style="text-align:center;"><img src="/images/cp.png" style="width:50px;"/><br/><%=cpPrice[i]%><br/>
              <span style="font-size:11px;color:red;vertical-align:middle;">(1매당 <%=cpUnitPrice[i]%>원)</span></td>
            <td><a href="<%=cpLinkUrl[i]%>" target="_blank"><img src="<%=cpDealImg[i]%>" style="width:100px;"/></a></td>
        </tr>
<%
            }
        }

        if (tmDealImg != '') {
          var limit = 5;
          if (tmDealImg.length < 6) {
              limit = tmDealImg.length;
          }
          for(var i = 0; i < limit; i++) {
%>
        <tr>
            <td style="text-align:center;vertical-align:middle;"><img src="/images/tm.png" style="width:90px;"/><br/><%=tmPrice[i]%></td>
            <td><a href="<%=tmLinkUrl[i]%>" target="_blank"><img src="<%=tmDealImg[i]%>" style="width:100px;"/></a></td>
        </tr>
<%
            }
        }
%>
    </table>
    </div>
</div><!-- /content -->

<script>
$(function () {
  var startAt = 0;

  if ('<%=keyword%>' == '슈퍼대디기저귀') {
     startAt = 0;
  } else if ('<%=keyword%>' == '네이처메이드기저귀') {
     startAt = 1;
  } else if ('<%=keyword%>' == '페넬로페기저귀') {
     startAt = 2;
  } else if ('<%=keyword%>' == '하기스기저귀') {
     startAt = 3;
  } else if ('<%=keyword%>' == '팸퍼스기저귀') {
     startAt = 4;
  } else if ('<%=keyword%>' == '군기저귀') {
     startAt = 5;
  } else if ('<%=keyword%>' == '보솜이기저귀') {
     startAt = 6;
  } else if ('<%=keyword%>' == '마미포코기저귀') {
     startAt = 7;
  } else if ('<%=keyword%>' == '네이쳐러브메레기저귀') {
     startAt = 8;
  }

  console.log('<%=keyword%>');
  $('.frame').sly({
    horizontal: 1,
    itemNav: 'forceCentered',
    smart: 1,
    activateOn: 'click',
    scrollBy: 1,
    touchDragging: 1,
    mouseDragging: 1,
    releaseSwing: 1,
    elasticBounds: 1,
    dynamicHandle: 1,
    clickBar: 1,
    scrollBar: $('.scrollbar'),
    dragHandle: 1,
    speed: 600,
    startAt : startAt
  });
});
</script>
